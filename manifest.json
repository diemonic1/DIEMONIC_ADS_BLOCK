{
   "content_scripts": [ 
      {
         "all_frames": true,
         "js": [ "content.js" ],
         "matches": [ "https://yandex.ru/search/*", "https://yandex.ru/video/search?*", "https://yandex.ru/images/search?*", "https://yandex.ru/maps/*", "https://yandex.ru/pogoda/*", "https://yandex.ru/*" ]
      }, 
      {
         "matches": [ "https://mail.yandex.ru/*"],
         "js": [ "jquery-3.5.1.min.js", "mail_ads.js"],
         "run_at": "document_end"
      }
   ],
   "description": "Кастомный адблокер",
   "host_permissions": [ "https://yandex.ru" ],
   "key": "",
   "manifest_version": 3,
   "name": "DIEMONIC ADS BLOCK",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.1",
   "options_ui": {
      "page": "options.html"
   },
   "permissions": ["storage"],
      "icons": {
         "16": "icon.png",
         "32": "icon.png",
         "48": "icon.png",  
         "64": "icon.png",
         "128": "icon.png"
      }
}
